{
  "name": "tmr-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "engines": {
    "node": ">=18.17.1 <20.11.1"
  },
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "start": "expo start",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "rebuild": "rm -rf node_modules && yarn"
  },
  "dependencies": {
    "@eslint/create-config": "1.1.4",
    "@expo-google-fonts/montserrat": "^0.2.3",
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^6.1.7",
    "@tamagui/animations-moti": "^1.100.3",
    "@tamagui/config": "^1.100.3",
    "@tamagui/font-inter": "1.74.8",
    "@tamagui/linear-gradient": "^1.100.3",
    "@tamagui/lucide-icons": "^1.100.3",
    "@tamagui/metro-plugin": "^1.100.3",
    "@tamagui/react-native-media-driver": "1.74.8",
    "@tamagui/shorthands": "1.74.8",
    "@tamagui/themes": "1.74.8",
    "expo": "^51.0.13",
    "expo-constants": "~16.0.1",
    "expo-font": "~12.0.7",
    "expo-haptics": "~13.0.1",
    "expo-image": "^1.12.11",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.14",
    "expo-screen-orientation": "~7.0.5",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
    "moti": "^0.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.2",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "tamagui": "^1.100.3"
