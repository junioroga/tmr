{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "~/*": ["./*"]
    }
  },
  "include": [
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "jest.setup.js",
    "jest.config.js"
  ]
}
